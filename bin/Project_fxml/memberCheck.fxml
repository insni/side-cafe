<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Button?>

<AnchorPane fx:id="anchorPane" prefWidth="1400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Project_Controller.MemberCheckController">
	<children>
		<TableView fx:id="tableView" prefHeight="700.0" prefWidth="1250.0">
			<columns>
				<TableColumn prefWidth="100.0" style="-fx-alignment: center;" text="이름" />
				<TableColumn prefWidth="100.0" style="-fx-alignment: center;" text="비밀번호" />
				<TableColumn prefWidth="100.0" style="-fx-alignment: center;" text="이름" />
				<TableColumn prefWidth="150.0" style="-fx-alignment: center;" text="연락처" />
				<TableColumn prefWidth="200.0" style="-fx-alignment: center;" text="이메일" />
				<TableColumn prefWidth="200.0" style="-fx-alignment: center;" text="질문" />
				<TableColumn prefWidth="200.0" style="-fx-alignment: center;" text="질문의 답" />
            <TableColumn prefWidth="199.0" style="-fx-alignment: center;" text="가입일" />

			</columns>
		</TableView>
      <VBox layoutX="1250.0" layoutY="43.0" prefHeight="200.0" prefWidth="250.0">
         <children>
            <TextField fx:id="textField" alignment="TOP_LEFT" style="-fx-alignment: center;">
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </VBox.margin>
            </TextField>
            <HBox prefHeight="100.0" prefWidth="250.0">
               <children>
                  <Button fx:id="btnCheck" mnemonicParsing="false" onAction="#check" prefHeight="20.0" prefWidth="80.0" text="조회">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="btnCheckAll" alignment="CENTER" mnemonicParsing="false" onAction="#checkAll" prefHeight="20.0" prefWidth="80.0" text="전체조회">
                     <HBox.margin>
                        <Insets left="50.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
      <Label alignment="CENTER" layoutX="1250.0" layoutY="-1.0" prefHeight="30.0" prefWidth="250.0" text="회원조회" />
	</children>
</AnchorPane>
